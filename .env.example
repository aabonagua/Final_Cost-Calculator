# Email address to send Nooko alert emails to - SEPARATE ADDRESSES WITH COMMAS
NOOKO_ALERT_EMAIL_TO="support@nooko.ai"

# Internal token for authentication with Nooko services
NOOKO_INTERNAL_TOKEN="INTERNAL_TOKEN_HERE"

# Set to "1" to enable dry run mode (emails will not be sent)
NOOKO_ALERT_EMAIL_DRY_RUN="0"

# Set to "1" to enable debug mode (more verbose logging)
NOOKO_ALERT_EMAIL_DEBUG="1"
